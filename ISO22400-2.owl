                <owl:onProperty rdf:resource="http://www.hsu-ifa.de/ontologies/ISO22400-2#Timing"/>
                <owl:allValuesFrom>
                    <rdfs:Datatype>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdfs:Datatype>
                                <owl:oneOf>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first>demand-oriented</rdf:first>
                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                    </rdf:Description>
                                </owl:oneOf>
                            </rdfs:Datatype>
                            <rdfs:Datatype>
                                <owl:oneOf>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
