                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.hsu-ifa.de/ontologies/ISO22400-2#Elements"/>
                    <rdf:Description rdf:about="http://www.hsu-ifa.de/ontologies/ISO22400-2#KeyPerformanceIndicator"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:comment>The basic unit or dimension in which the KPI or Parameter is expressed.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.hsu-ifa.de/ontologies/ISO22400-2#UserGroup -->

    <owl:DatatypeProperty rdf:about="http://www.hsu-ifa.de/ontologies/ISO22400-2#UserGroup">
        <rdfs:domain rdf:resource="http://www.hsu-ifa.de/ontologies/ISO22400-2#KeyPerformanceIndicator"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
